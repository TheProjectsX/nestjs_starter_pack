    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.6.1",
    "cookie-parser": "^1.4.7",
    "csrf-csrf": "^4.0.0",
    "date-fns": "^4.1.0",
