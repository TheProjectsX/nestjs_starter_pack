    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.9",
    "@types/passport-facebook": "^3.0.3",
